{
  "name": "regl-d3",
  "version": "0.0.1",
  "description": "A testbed for discovering best practices working with regl and d3.",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js --open --live",
    "build": "browserify index.js -o bin.js"
  },
  "repository": {
